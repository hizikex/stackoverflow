{
  "env": {
    "commonjs": true,
    "node": true
  },
  "extends": ["airbnb-typescript", "plugin:prettier/recommended"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2016,
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "comma-style": ["error", "last"],
    "comma-dangle": ["off"],
    "quotes": ["error", "double"]
  }
}
